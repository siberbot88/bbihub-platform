                                    "// Save token for subsequent requests",
                                    "if (pm.response.code === 201) {",
                                    "    var jsonData = pm.response.json();",
                                    "    pm.environment.set(\"access_token\", jsonData.data.token);",
                                    "    pm.environment.set(\"user_id\", jsonData.data.user.id);",
